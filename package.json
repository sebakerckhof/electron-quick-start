{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "node ./tools/preinstall",
    "barco:install": "sh ./install.sh",
    "start": "electron-forge start",
    "package": "./sandbox.sh && electron-forge package",
    "test": "ELECTRON_PATH=./node_modules/.bin/electron ./node_modules/.bin/electron-mocha 'src/**/*.spec.js'"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "electron-compile": "6.4.4",
    "electron-debug": "3.0.1",
    "electron-is-dev": "1.1.0"
  },
  "devDependencies": {
    "@barco/electron-forge": "5.2.4",
    "@barco/electron-prebuilt-compile": "6.0.7",
    "babel-preset-env": "1.7.0"
  }
}
